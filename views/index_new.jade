        app.controller('VRController', ['$scope', '$http', '$window','$firebase','$firebaseObject', '$firebaseArray',  function ($scope, $http, $window, $firebase, $firebaseObject, $firebaseArray) {

            var ref = firebase.database().ref().child("reserves");
            var fobj = $firebaseObject(ref);

            $scope.panels ={};
            $scope.programs = {};
            $scope.reserves = [];
            $scope.sort_key = "program";
            $scope.sort_order = "desc";

            $scope.loadReserves = function () {
                fobj.$loaded().then(function (loaded) {

                    $scope.reserves = [];

                    angular.forEach(loaded.form, function (val, key) {
                        val.key = key;
                        $scope.reserves.push(val);

                        if(res_counts[val.program]==undefined){
                            res_counts[val.program]=1;
                        }else{
                            res_counts[val.program]++;
                        }
                    });


                    /*
